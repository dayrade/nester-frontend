{
  "name": "nester-platform",
  "version": "1.0.0",
  "description": "AI-powered real estate marketing platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:server": "cd server && npm run dev",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "install:all": "npm install && cd server && npm install",
    "clean:all": "rm -rf node_modules package-lock.json && cd server && rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^9.39.0",
    "@sentry/react": "^9.39.0",
    "@supabase/auth-helpers-nextjs": "^0.8.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "^4.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "framer-motion": "^10.16.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "react-pageflip": "^2.0.3",
    "recharts": "^2.8.0",
    "sharp": "^0.32.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "ts-jest": "^29.4.0"
  },
  "keywords": [
    "real-estate",
    "ai",
    "marketing",
    "automation",
    "social-media"
  ],
  "author": "Nester Platform",
  "license": "MIT"
}
